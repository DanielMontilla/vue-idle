{
   "$schema": "../node_modules/@tauri-apps/cli/schema.json",
   "build": {
      "beforeBuildCommand": "npm run build",
      "beforeDevCommand": "npm run dev",
      "devPath": "http://localhost:8080",
      "distDir": "../dist"
   },
   "package": {
      "productName": "vue-idle",
      "version": "0.1.0"
   },
   "tauri": {
      "allowlist": {
         "fs": {
            "all": true,
            "scope": ["$CONFIG/vue-idle/*"]
         },
         "all": true
      },
      "bundle": {
         "active": true,
         "category": "DeveloperTool",
         "copyright": "",
         "deb": {
            "depends": []
         },
         "externalBin": [],
         "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
         ],
         "identifier": "com.vue-idle0",
         "longDescription": "",
         "macOS": {
            "entitlements": null,
            "exceptionDomain": "",
            "frameworks": [],
            "providerShortName": null,
            "signingIdentity": null
         },
         "resources": [],
         "shortDescription": "",
         "targets": "all",
         "windows": {
            "certificateThumbprint": null,
            "digestAlgorithm": "sha256",
            "timestampUrl": ""
         }
      },
      "security": {
         "csp": null
      },
      "updater": {
         "active": false
      },
      "windows": [
         {
            "title": "Vue Idle",
            "fullscreen": false,
            "fileDropEnabled": false,
            "height": 900,
            "width": 1280,
            "resizable": false,
            "decorations": false,
            "transparent": true
         }
      ]
   }
}
